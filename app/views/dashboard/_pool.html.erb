<div class="show-the-daters-backers" >
  <% if current_dater.pool.empty? %>

    <div class="alert alert-warning" role="alert" id="pool-is-empty">
      <h4 class="">Your Dating Pool is Empty!</h4>
      <p>Message your Backers! Tell them to pull it together and find you a date!</p>
   </div>

  <% else %>
    <div id='gallery-cards-container'>
    <% current_dater.pool.each do |match| %>
            <div class="card dater-card">
            <% if current_user.image %>
            <%= link_to image_tag(match.first.user.image.url, class:'card-img-top'), user_path(match.first.user) %>
            <% end %>
                <div class="card-body"id="card-body" >
                    <h5 class="card-title" id="card-body"><%= match[0].f_name %></h5>
                </div>
            </div>
        <% end %>
    </div>
    <% end%>
</div>
