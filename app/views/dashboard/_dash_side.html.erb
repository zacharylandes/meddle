


<div class='card card-profile text-center col-md-2' id='dash-side' >
  <div class = 'card-header' id = 'dash-side-header'> You</div>
  <img alt='' class='card-img-top' src='https://s3.amazonaws.com/pytdating/andrea-reiman-417352-unsplash.jpg'>
  <div class='card-block'>
    <% if current_user.image %>
      <%= image_tag current_user.image.url, class:'card-img-profile' %>
    <% end %>
    <div class='card-title'>
      <h6> <%= current_user.f_name%> </h6>
      <p> <%= current_user.l_name%></p> 
    </div>
    <div class="card-body ">
      <h6>Location</h6><br>
      <p><%= ZipCodes.identify("#{current_user.daters.first.trait.zip}")[:city]%></p>
    </div>
  </div>
</div>


