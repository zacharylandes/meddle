

<div class = 'dashboard-container'>
    <div class="card col-md-2 dash-side " >
    <%= image_tag('blackwhite.jpg', id:'dash-dater-img')%>
    <div class="card-body ">
        <%= link_to 'Profile',user_path(current_user), class:"primary-btn btn-md"  %>
        <%= link_to 'Gallery',user_path(current_user), class:"primary-btn btn-md"%>
         <%= link_to 'Messages',conversations_path, class:"primary-btn btn-md" %>
</div>
  </div>

    <div class ='col-md-offset-1 col-md-10'>
        <div class = 'dash-card-container'>
        <% if params['tab'] = 'backer' && params['tab'] != 'dater' %>
            <%= render 'backer_tab' %>
            <%print 'params',params%>
         <% else %>
            <%= render 'dater_tab'%>
         <% end %>
         </div>
     </div>
</div>

